    <section>
        <div class="content_contato">
            <div class="container">
                <div class="row">
                    <div class="col-md-6">
                        <div class="infos_contato_lateral">
                            <h3>INFORMAÇÕES DE CONTATO</h3>



                            <ul>
                                <li><a href="tel:+551139001054"><i class="fa fa-phone"></i> Telefone: (11) 3900-1054</a></li>
