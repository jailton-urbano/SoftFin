                        <div class="form_contato">
                            <form>
                                <div class="row">
                                    <div class="col-md-6">
                                        <input type="text" id="contnome" name="nome" placeholder="Seu nome">
                                    </div>

                                    <div class="col-md-6">
                                        <input type="email" id="contemail" name="email" placeholder="E-mail">
                                    </div>

                                    <div class="col-md-6">
                                        <input type="text" id="contempresa" name="empresa" placeholder="Empresa">
                                    </div>

                                    <div class="col-md-6">
                                        <input type="tel" id="conttelefone" class="tel" name="telefone" placeholder="Telefone">
                                    </div>

                                    <div class="col-md-12">
                                        <input type="text" id="contassunto" name="assunto" placeholder="Assunto">
                                    </div>


                                </div>

                                <textarea id="contmensagem" placeholder="Sua mensagem" style="height:200px"></textarea>
                                <br />
                                <div class="simple-msg"></div>
                                <div id="error_message" class="ajax_response msg_erro" style="float:left"></div>
                                <div id="success_message" class="ajax_response msg_sucesso" style="float:left;"></div>
                                <br />
                                <br />
                                <br />
                                <input id="contbtnEnviar" type="submit" value="Enviar" class="btn_form_contato" style="cursor:pointer">


