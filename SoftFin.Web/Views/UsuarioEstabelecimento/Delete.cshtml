@model SoftFin.Web.Models.UsuarioEstabelecimento

@{
    ViewBag.Title = "Relacionamento Usuário X Estabelecimento - Exclusão";
}

@RenderPage("~/Views/Shared/_BotoesExclusao.cshtml")

<fieldset>
        <div class="container">
             <div class="row">
                <div class="col-md-4 form-group">
                    <div class="editor-label">
                        @Html.LabelFor(model => model.usuario_id)
                    </div>
                    <div class="editor-field">
                        @Html.DropDownListFor(model => model.usuario_id, ViewData["Usuarios"] as SelectList, null, new { @class = "form-control" })   
                        @Html.ValidationMessageFor(model => model.usuario_id)
                    </div>
                </div>
                <div class="col-md-4 form-group">
                    <div class="editor-label">
                        @Html.LabelFor(model => model.estabelecimento_id)
                    </div>
                    <div class="editor-field">
                        @Html.DropDownListFor(model => model.estabelecimento_id, ViewData["Estabelecimentos"] as SelectList, null, new { @class = "form-control" })   
                        @Html.ValidationMessageFor(model => model.estabelecimento_id)
                    </div>
                </div>
             </div>
        </div>
</fieldset>


@RenderPage("~/Views/Shared/_BotoesExclusao.cshtml")

<script>
    $(document).ready(function () {
        Metronic.init(); // init metronic core components
        Layout.init(); // init current layout
        QuickSidebar.init(); // init quick sidebar
    });
    $("fieldset input").attr("disabled", "disabled");
    $("select").attr("disabled", "disabled");  
</script>