                    <div class="panel-body">
                        <div class='container-fluid'>
                            <div class='row'>
                                <div class='col-md-9 form-group'>
                                </div>
                                <div class='col-md-3 form-group'>
                                    <span class="input-group-btn">
                                        <button type="button"
                                                ng-click="Salvar()"
                                                class="btn btn-primary"
                                                ng-show="!ModoConsulta">
                                            <span class="glyphicon glyphicon-ok"></span> {{msgSalvar}}
                                        </button>
                                    </span>
                                    <span class="input-group-btn">
                                        <button type="button"
                                                ng-click="Voltar()"
                                                class="btn btn-default">
                                            <span class="glyphicon glyphicon-log-out"></span> Voltar
                                        </button>
                                    </span>
                                </div>
                            </div>
                            <div class="alert alert-info" ng-show="showOK">
                                {{msgOK}}
                            </div>
                            <div class="alert alert-danger" ng-show="showNOK">
                                {{msgNOK}}
                                <ul>
                                    <li ng-repeat="item in Erros">{{item}}</li>
                                </ul>
                            </div>


                            <div class='row'>
                                <div class='col-md-2 form-group'>
                                    <label>Categoria Pessoa</label>
                                    <select ng-disabled="ModoConsulta" class="form-control" ng-model="pessoa.CategoriaPessoa_ID" ng-options="o.Value as o.Text for o in ListaCategorias">
                                        <option value="">--Selecione--</option>
                                    </select>
                                </div>
                                <div class='col-md-2 form-group'>
                                    <label>CNPJ/CPF</label>
                                    <input ng-disabled="ModoConsulta" type="text" ng-model="pessoa.cnpj" class="form-control" ng-maxlength="20" ui-br-cnpj-mask ng-show="pessoa.CategoriaPessoa_ID != 1" />
                                    <input ng-disabled="ModoConsulta" type="text" ng-model="pessoa.cnpj" class="form-control" ng-maxlength="20" ui-br-cpf-mask ng-show="pessoa.CategoriaPessoa_ID == 1" />
                                    <span class="input-group-btn">
                                        <a ng-show="!ModoConsulta" ng-click="BuscaSefaz(pessoa.cnpj)" class="btn btn-default" role="button">Pesquisar</a>
                                    </span>
                                </div>
                                <div class='col-md-4 form-group'>
                                    <label>Nome Pessoa</label>
                                    <input ng-disabled="ModoConsulta" type="text" ng-model="pessoa.nome" class="form-control" required ng-maxlength="150" />
                                </div>
                                <div class='col-md-4 form-group'>
                                    <label>Razão Social</label>
                                    <input ng-disabled="ModoConsulta" type="text" ng-model="pessoa.razao" class="form-control" ng-maxlength="150" />
                                </div>
                            </div>
                            <div class='row'>
                                <div class='col-md-3 form-group'>
                                    <label>Tipo Pessoa</label>
                                    <select ng-disabled="ModoConsulta" class="form-control" ng-model="pessoa.TipoPessoa_ID" ng-options="o.Value as o.Text for o in ListaTipoPessoas">
                                        <option value="">--Selecione--</option>
                                    </select>
                                </div>

                                <div class='col-md-3 form-group'>
                                    <label>Unidade de Negócio</label>
                                    <select ng-disabled="ModoConsulta" class="form-control" ng-model="pessoa.UnidadeNegocio_ID" ng-options="o.Value as o.Text for o in ListaUnidades">
                                        <option value="">--Selecione--</option>
                                    </select>
                                </div>
                                <div class='col-md-3 form-group' title="Se deixar vazio o sistema cria um novo pra você">
                                    <label>Código(Vazio gera automático)</label>
                                    <input ng-disabled="ModoConsulta" type="text" ng-model="pessoa.codigo" class="form-control" ng-maxlength="20" title="Se deixar vazio o sistema cria um novo pra você" />
                                </div>
                            </div>
                            <div class='row'>
                                <div class='col-md-3 form-group'>
                                    <label>Telefone Fixo</label>
                                    <input ng-disabled="ModoConsulta" type="text" ng-model="pessoa.TelefoneFixo" class="form-control" ng-maxlength="20" />
                                </div>
                                <div class='col-md-3 form-group'>
                                    <label>Celular</label>
                                    <input ng-disabled="ModoConsulta" type="text" ng-model="pessoa.Celular" class="form-control" ng-maxlength="20" />
                                </div>

                                <div class='col-md-3 form-group'>
                                    <label>Incrição Estadual</label>
                                    <input ng-disabled="ModoConsulta" type="text" ng-model="pessoa.inscricao" class="form-control" ng-maxlength="20" />
                                </div>
                                <div class='col-md-3 form-group'>
                                    <label>CCM</label>
                                    <input ng-disabled="ModoConsulta" type="text" ng-model="pessoa.ccm" class="form-control" ng-maxlength="20" />
                                </div>
                                <div class='col-md-7 form-group'>
                                    <label>E-Mail</label>
                                    <input ng-disabled="ModoConsulta" type="text" ng-model="pessoa.eMail" class="form-control" ng-maxlength="150" />
                                </div>
                            </div>


                            <div id="tabs">
                                <ul class="nav nav-tabs">
