<div ng-app="SOFTFIN" id="DivTotal" style="display:none" ng-cloak>
    <div ng-controller="MestreEntidade">
        <div id="loadGrid" style="text-align: center;" class="loading_img" ng-show="showloadGrid">
            <img src="~/Content/images/loading_bar_animated.gif" />
        </div>
        
        <script type="text/ng-template" id="modalExcluir.html">
            <div>
                <div class="modal-header">
                    <h3>Excluir a Pessoa ?</h3>
                </div>
                <div class="modal-body">
                    <div class='container'>
                        <div class='row'>
                            <div class='col-md-12 form-group'>
                                <label>{{item.codigo}} - {{item.nome}} - {{item.razao}} - {{item.cnpj}}</label>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <div class="alert alert-danger" ng-show="showNOK">
                        {{msgNOK}}
                        <ul>
                            <li ng-repeat="item in Erros">{{item}}</li>
                        </ul>
                    </div>
                    <button class="btn btn-primary" ng-click="ok()">
                        <span class="glyphicon glyphicon-ok"></span>
                        OK
                    </button>
                    <button class="btn btn-default" ng-click="cancel()">
                        <span class="glyphicon glyphicon-log-out"></span>
                        Voltar
                    </button>
                </div>
            </div>
        </script>

        <script type="text/ng-template" id="modalAdicionarFoto.html">
            <div>
                <div class="modal-header">
                    <h3>Selecione o arquivo de imagem para upload</h3>
                </div>
                <div class="modal-body">
                    <div class='container-fluid'>
                        <div class="row">
                            <div class='col-md-3 form-group'>
                                <label> Arquivo </label>
                                <input type="file" id="newPhotos" accept="image/*" ng-model="arquivo" has-files="hasFiles">
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <div class="alert alert-info" ng-show="showOK">
                            {{msgOK}}
                        </div>
                        <div class="alert alert-danger" ng-show="showNOK">
                            {{msgNOK}}
                            <ul>
                                <li ng-repeat="item in Erros">{{item}}</li>
                            </ul>

                        </div>
                        
                        <button class="btn btn-primary" ng-click="ok()">
                            <span class="glyphicon glyphicon-ok"></span>
                            Carregar
                        </button>
                        <button class="btn btn-default" ng-click="cancel()">
                            <span class="glyphicon glyphicon-log-out"></span>
                            Voltar
                        </button>
                    </div>
                </div>
            </div>
        </script>

        <script type="text/ng-template" id="modalContato.html">
            <div>
                <div class="modal-header">
                    <h3>Contato</h3>
                </div>
                <div class="modal-body">
                    <div class='container-fluid'>
                        <div class='row'>
                            <div class='col-md-8 form-group'>
                                <label>Nome</label>
                                <input type="text" ng-model="contato.nome" class="form-control" ng-maxlength="75" required tabindex="1"/>
                            </div>
                            <div class='col-md-4 form-group'>
                                <label>Cargo / Relacionamento</label>
                                <input type="text" ng-model="contato.cargo" class="form-control" ng-maxlength="75" tabindex="2" />
                            </div>
                            <div class='col-md-8 form-group'>
                                <label>Email</label>
                                <input type="text" ng-model="contato.email" class="form-control" ng-maxlength="75" tabindex="3"/>
                            </div>
                            <div class='col-md-4'>
                                
                                <label>Recebe Cobrança</label>
                                <select class="form-control" ng-model="contato.RecebeCobranca" ng-disabled='ModoConsulta'
                                        ng-options="o.Value as o.Text for o in ListaSimNao">
                                    <option value="">--Selecione--</option>
                                </select>
                            </div>

                            <div class='col-md-6 form-group'>
                                <label>Telefone</label>
                                <input type="text" ng-model="contato.telefone" class="form-control" ng-maxlength="75" tabindex="5"/>
                            </div>
                            <div class='col-md-6 form-group'>
                                <label>Celular</label>
                                <input type="text" ng-model="contato.celular" class="form-control" ng-maxlength="75" tabindex="6"/>
                            </div>
                            <div class='col-md-6 form-group'>
                                <label>Observação</label>
                                <input type="text" ng-model="contato.observacao" class="form-control" ng-maxlength="150"  tabindex="7"/>
                            </div>

                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <div class="alert alert-danger" ng-show="showNOK">
                        {{msgNOK}}
                        <ul>
                            <li ng-repeat="item in Erros">{{item}}</li>
                        </ul>
                    </div>
                    <button class="btn btn-primary" ng-click="ok()"  tabindex="8">
                        <span class="glyphicon glyphicon-ok"></span>
                        Salvar
                    </button>
                    <button class="btn btn-default" ng-click="cancel()"  tabindex="9">
                        <span class="glyphicon glyphicon-log-out"></span>
                        Voltar
                    </button>
                </div>
            </div>
        </script>

        <div id="Grid" class="container-fluid" ng-show="showGrid">
            <div class='panel-body'>
                <form class="form-search">
                    <div class='container'>

                        <div class='col-md-3 form-group'>
                            <label>Pesquisa : </label>
                            <input type="text" class="form-control" ng-model="procurar" />
                        </div>
                        
                    </div>

                </form>
            </div>

            <dir-pagination-controls max-size="10" boundary-links="true"></dir-pagination-controls>
            <table class="table table-bordered">
                <thead>
                    <tr>
                        <th ng-show="showMostraTodos">


                            <button style="border-right-width: 0px; padding-right: 49px;"
                                    ng-click="LoadListaPessoas()"
                                    class="btn btn-warning"
                                    title="Atualiza a lista na tela">
                                <span class="glyphicon glyphicon-refresh"></span> Atualizar
                            </button>

                            <button style="margin-top: 4px; padding-right: 72px;"
                                    ng-click="Novo()"
                                    ng-show="EditAccess"
                                    title="Nova pessoa"
                                    class="btn btn-primary">
                                <span class="glyphicon glyphicon-plus"></span> Novo
                            </button>

                            <button type="button" style="margin-top: 4px; border-right-width: 0px; padding-right: 73px;"
                                    ng-click="Excel()"
                                    title="Gera um aquivo do Excel"
                                    class="btn btn-success">
                                <span class="glyphicon glyphicon-file"></span> Excel
                            </button>

                            <button type="button" style="margin-top: 4px; border-right-width: 0px; padding-right: 31px;"
                                    ng-click="Importar()"
                                    title="Importar arquivo Excel/CSV"
                                    class="btn btn-danger">
                                <span class="glyphicon glyphicon-open-file"></span> Importação
                            </button>
                        </th>
                        <th>Identificação</th>
                        <th>Dados</th>
                    </tr>
                </thead>
                <tbody>
                    <tr dir-paginate="item in ListaPessoas|filter:procurar|itemsPerPage:7|orderBy:sortKey:reverse" class="{{item.active}}">
                        <td style="width: 40px;" ng-show="showMostraTodos">



                            <button style="margin-top: 4px; border-right-width: 0px; padding-right: 50px;"
                                    ng-click="Detalhar(item)"
                                    class="btn btn-primary"
                                    title="Detalhes da pessoa">
                                <span class="glyphicon glyphicon-list-alt"></span> Detalhar
                            </button>


                            <button style="margin-top: 4px; border-right-width: 0px; padding-right: 68px;"
                                    ng-click="Alterar(item)"
                                    ng-show="EditAccess"
                                    class="btn btn-primary"
                                    title="Editar pessoa">
                                <span class="glyphicon glyphicon-edit"></span> Editar
                            </button>

                            <button style="margin-top: 4px;border-right-width: 0px; padding-right: 63px;"
                                    type="button"
                                    ng-click="Excluir(item)"
                                    ng-show="EditAccess"
                                    class="btn btn-danger"
                                    title="Excluir pessoa">
                                <span class="glyphicon glyphicon-remove"></span> Excluir
                            </button>
                            <button type="button" style="border-top-width: 0px; margin-top: 4px; padding-right: 77px;"
                                    ng-click="AdicionarFoto(item)"
                                    class="btn btn-warning"
                                    title="Fazer upload da foto da pessoa">
                                <span class="glyphicon glyphicon-user"></span> Foto
                            </button>



                        </td>

                        <td ng-show="item.id == idSelecionado || showMostraTodos">
                            <b>
                                <span ng-click="ordenar('codigo')"> Código </span> / <span ng-click="ordenar('nome')">Nome </span>/ <span ng-click="ordenar('razao')">Razão Social: </span>
</b>
                            <br />
                            {{item.codigo}} / {{item.nome}} / {{item.razao}}
                            <br />
                            <b>
                                <span ng-click="ordenar('cnpj')"> CNPJ/CPF: </span>
</b>
               
                            {{item.cnpj}}
                            <div ng-show="(item.Foto != null)">
                                <br />
                                <b>Foto</b>
                                <br />
                                <img src="{{item.Foto}}" style="height: 200px; width: 200px;" />
                            </div>
                        </td>
                        <td ng-show="item.id == idSelecionado || showMostraTodos">
                            <b>
                                <span ng-click="ordenar('endereco')">Endereço</span>
</b>
                            <br />
                            {{item.endereco}},{{item.numero}} {{item.complemento}} {{item.bairro}} {{item.cidade}} {{item.uf}} {{item.cep}}
                            <br />
                            <b>
                                <span ng-click="ordenar('eMail')">Email </span> / <span ng-click="ordenar('TelefoneFixo')">Telefone </span>/ <span ng-click="ordenar('Celular')">Celular </span>
</b>
                            <br />
                            {{item.eMail}} / {{item.TelefoneFixo}} / {{item.Celular}}
                            <br />
                            <b>
                                <span ng-click="ordenar('bancoConta')">Banco </span> / <span ng-click="ordenar('agenciaConta')">Agência </span>/ <span ng-click="ordenar('agenciaConta')">Conta</span>
                            </b>
                            <br />
                            {{item.bancoConta}} / {{item.agenciaConta}}-{{item.agenciaDigito}} / {{item.agenciaConta}}-{{item.digitoContaBancaria}}

                        </td>
                    </tr>
                </tbody>
            </table>
            <dir-pagination-controls max-size="10" boundary-links="true"></dir-pagination-controls>


            <div class="alert alert-info" ng-show="showOK" id="showOKTop">
                {{msgOK}}
            </div>
            <div class="alert alert-danger" ng-show="showNOK">
                {{msgNOK}}
                <ul>
                    <li ng-repeat="item in Erros">{{item}}</li>
                </ul>
            </div>
        </div>

        <div class="container-fluid" ng-show="showManut">

            <div class="panel panel-primary">
                <div class="panel-heading" ng-show="pessoa.id == 0">Manutenção de Dados de Pessoa</div>
