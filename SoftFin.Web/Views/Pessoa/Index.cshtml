                                            <div class='container-fluid'>

                                                <div class='row'>

                                                    <div class='col-md-2 form-group'>
                                                        <label>CEP</label>
                                                        <span class="input-group-btn">
                                                            <input ng-disabled="ModoConsulta" type="text" ng-model="pessoa.cep" class="form-control" ng-maxlength="14" ui-br-cep-mask />
                                                            <a ng-show="!ModoConsulta" ng-click="BuscaCEP(pessoa.cep)" class="btn btn-default" role="button">Pesquisar</a>
                                                        </span>
                                                    </div>
                                                    <div class='col-md-3 form-group'>
                                                        <label>Tipo Endereço</label>
                                                        <select ng-disabled="ModoConsulta" class="form-control" ng-model="pessoa.TipoEndereco_ID" ng-options="o.Value as o.Text for o in ListaTipoEnderecos">
                                                            <option value="">--Selecione--</option>
                                                        </select>
                                                    </div>
                                                    <div class='col-md-7 form-group'>
                                                        <label>Endereço</label>
                                                        <input ng-disabled="ModoConsulta" type="text" ng-model="pessoa.endereco" class="form-control" ng-maxlength="50" />
                                                    </div>
                                                </div>
                                                <div class='row'>
                                                    <div class='col-md-2 form-group'>
                                                        <label>Número</label>
                                                        <input ng-disabled="ModoConsulta" type="text" ng-model="pessoa.numero" class="form-control" ng-maxlength="20" />
                                                    </div>
                                                    <div class='col-md-3 form-group'>
                                                        <label>Complemento</label>
                                                        <input ng-disabled="ModoConsulta" type="text" ng-model="pessoa.complemento" class="form-control" ng-maxlength="50" />
                                                    </div>
                                                    <div class='col-md-3 form-group'>
                                                        <label>Bairro</label>
                                                        <input ng-disabled="ModoConsulta" type="text" ng-model="pessoa.bairro" class="form-control" ng-maxlength="50" />
                                                    </div>
                                                    <div class='col-md-1 form-group'>
                                                        <label>UF</label>
                                                        <input ng-disabled="ModoConsulta" type="text" ng-model="pessoa.uf" class="form-control" ng-maxlength="2" />
                                                    </div>
                                                    <div class='col-md-3 form-group'>
                                                        <label>Cidade</label>
                                                        <input ng-disabled="ModoConsulta" type="text" ng-model="pessoa.cidade" class="form-control" ng-maxlength="30" />
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                </div>
                                    
                                    <div class="tab-pane fade in" id="tabBanco" aria-expanded="false">
                                        <div class="panel panel-primary">
                                            <div class='container-fluid'>


