        <script type="text/ng-template" id="modalContratoItem.html">
            <div>
                <div class="modal-header">
                    <h3>Item de Contrato</h3>

                </div>

                <div class="modal-body" ng-form="frmItemContrato">
                    <div class='row' ng-hide="ModoConsultaCI">
                        <div class='col-md-3 form-group'>
                            <label>Unidade de Negócio</label>
                            <select class="form-control" ng-model="unidade.UnidadeNegocio_Id"
                                    ng-options="o.Value as o.Text for o in ListaUnidadeNegocios" ng-required="true">
                                <option value="">--Selecione--</option>
                            </select>
                        </div>
                        <div class='col-md-3 form-group'>
                            <label>Descrição</label>
                            <input ng-disabled="ModoConsultaPA" type="text" ng-model="unidade.Descricao" class="form-control" ng-maxlength="50" />
                        </div>
                        <div class='col-md-3 form-group'>
                            <label>Valor</label>
                            <input ng-disabled="ModoConsultaPA" type="text" ng-model="unidade.Valor" class="form-control" required ui-number-mask="2" />
                        </div>
                        <div class='col-md-3 form-group'>
                            <button style="margin-top: 24px;border-right-width: 0px; padding-right: 50px;"
                                    type="button"
                                    ng-show="!ModoConsultaPA"
                                    ng-click="AdicionarUnidade()"
                                    title="Adicionar o lançamento"
                                    class="btn btn-success">
                                <span class="glyphicon glyphicon-plus-sign"></span> Adicionar
                            </button>
                        </div>
                    </div>
                    <div class="row">
                        <dir-pagination-controls max-size="10" boundary-links="true" pagination-id="gridSecundario"></dir-pagination-controls>
                        <table class="table table-bordered">
                            <thead>
                                <tr>
                                    <th>

                                    </th>
                                    <th>
                                        Unidade
                                    </th>
                                    <th>
                                        Descrição
                                    </th>
                                    <th>
                                        Valor
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr dir-paginate="item in contratoitem.ListaItemUnidades |orderBy:sortKey:reverseRec|filter:procurarRec|itemsPerPage:10" pagination-id="gridSecundario">
                                    <td>
                                        <button style="margin-top: 4px;border-right-width: 0px; padding-right: 50px;"
                                                type="button"
                                                ng-click="ExcluirUnidade(item)"
                                                ng-show="!ModoConsultaCI"
                                                title="Remove o lançamento"
                                                class="btn btn-danger">
                                            <span class="glyphicon glyphicon-remove"></span> Excluir
                                        </button>
                                    </td>
                                    <td>{{item.Unidade_Desc}}</td>
                                    <td>
                                        {{item.Descricao}}
                                    </td>
                                    <td>{{item.Valor | number:2}}</td>
                                </tr>
                            </tbody>
                        </table>
                        <dir-pagination-controls max-size="10" boundary-links="true" pagination-id="gridSecundario"></dir-pagination-controls>
                        <div class="alert alert-danger" ng-show="showMSGUnidade">
                            {{msgMSGUnidade}}
                        </div>
                    </div>
                    <div class='row'>
                        <div class='col-md-6 form-group'>
                            <label>Codigo/Pedido</label>
                            <input ng-disabled="ModoConsultaCI" type="text" ng-model="contratoitem.pedido" class="form-control" ng-required="true" ng-maxlength="100" />
                        </div>
                        <div class='col-md-6 form-group'>
                            <label>Valor</label>
                            <input ng-disabled="true" type="text" ng-model="contratoitem.valor" class="form-control" ng-required="true" ng-maxlength="100" ui-number-mask="2" />
                        </div>
                        <div class='col-md-3 form-group'>
                            <label>Tipo Contrato</label>
                            <select class="form-control" ng-model="contratoitem.tipoContratos_ID" ng-disabled="ModoConsultaCI"
                                    ng-options="o.Value as o.Text for o in ListaTipoContratos" ng-required="true">
                                <option value="">--Selecione--</option>
                            </select>
                        </div>
                        <div class='col-md-3 form-group'>
                            <label>Unidade Faturamento</label>
                            <select class="form-control" ng-model="contratoitem.unidadeNegocio_ID" ng-disabled="ModoConsultaCI"
                                    ng-options="o.Value as o.Text for o in ListaUnidadeNegocios" ng-required="true">
                                <option value="">--Selecione--</option>
                            </select>
                        </div>
                        <div class='col-md-3 form-group'>
                            <label>Produto Serviço</label>
                            <select class="form-control" ng-model="contratoitem.itemProdutoServico_ID" ng-disabled="ModoConsultaCI"
                                    ng-options="o.Value as o.Text for o in ListaProdutoServicos" ng-required="true">
                                <option value="">--Selecione--</option>
                            </select>
                        </div>
                        <div class='col-md-3 form-group'>
                            <label>Tabela Preço</label>
                            <select class="form-control" ng-model="contratoitem.tabelaPreco_ID" ng-disabled="ModoConsultaCI"
                                    ng-options="o.Value as o.Text for o in ListaTabelaPrecos" ng-required="true">
                                <option value="">--Selecione--</option>
                            </select>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <div class="alert alert-danger" ng-show="showNOK">
                        {{msgNOK}}
                        <ul>
                            <li ng-repeat="item in Erros">{{item}}</li>
                        </ul>
                    </div>
                    <button class="btn btn-primary" ng-click="ok()">
                        <span class="glyphicon glyphicon-ok"></span>
                        Salvar
                    </button>
                    <button class="btn btn-default" ng-click="cancel()">
                        <span class="glyphicon glyphicon-log-out"></span>
                        Voltar

                    </button>

                </div>
            </div>
        </script>

        <script type="text/ng-template" id="modalParcela.html">
            <div>
                <div class="modal-header">
                    <h3>Parcela</h3>
                </div>
                <div class="modal-body">
                    <div class='row'>
                        <div class='col-md-2 form-group' ng-show="parcela.id == 0">
                            <label>Recorrente</label>
                            <select class="form-control" ng-model="parcela.recorrente" ng-disabled="ModoConsultaPI"
                                    ng-options="o.Value as o.Text for o in ListaSimNao" required>
                                <option value="">--Selecione--</option>
                            </select>
                        </div>
                        <div class='col-md-2 form-group'>
                            <label>Parcela</label>
                            <input ng-disabled="ModoConsultaPA" type="text" ng-model="parcela.parcela" class="form-control" required ng-maxlength="4" />
                        </div>
                        <div class='col-md-4 form-group'>
                            <label>Descrição</label>
                            <input ng-disabled="ModoConsultaPA" type="text" ng-model="parcela.descricao" class="form-control" required ng-maxlength="100" />
                        </div>
                        <div class='col-md-4 form-group'>
                            <label>Valor</label>
                            <input ng-disabled="ModoConsultaPA" type="text" ng-model="parcela.valor" class="form-control" required ng-maxlength="100" ui-number-mask="2" />
                        </div>
                        <div class='col-md-4 form-group' datetimez ng-model="parcela.data">
                            <label>Data</label>
                            <span class="add-on">
                                <i data-time-icon="icon-time" data-date-icon="icon-calendar"></i>
                            </span>
                            <input data-format="dd/MM/yyyy" type="text" class="form-control" ng-model="parcela.data" required ui-date-mask ng-disabled="ModoConsultaPA" />

                        </div>
                        <div class='col-md-4 form-group' datetimez ng-model="parcela.DataVencimento">
                            <label>Vencimento</label>
                            <span class="add-on">
                                <i data-time-icon="icon-time" data-date-icon="icon-calendar"></i>
                            </span>
                            <input data-format="dd/MM/yyyy" type="text" class="form-control" ng-model="parcela.DataVencimento" required ui-date-mask ng-disabled="ModoConsultaPA" />
                        </div>


                        <div class='col-md-12 form-group'>
                            <label>Operações</label>
                            <select class="form-control" ng-model="parcela.operacao_id" ng-disabled="ModoConsultaPA"
                                    ng-options="o.Value as o.Text for o in ListaOperacoes" required>
                                <option value="">--Selecione--</option>
                            </select>
                        </div>

                        <div class='col-md-4 form-group'>
                            <label>Tipo Faturamento</label>
                            <select class="form-control" ng-model="parcela.TipoFaturamento" ng-disabled="ModoConsultaPA"
                                    ng-options="o.Value as o.Text for o in ListaTipoFaturamento" required>
                                <option value="">--Selecione--</option>
                            </select>
                        </div>

                        <div class='col-md-4 form-group'>
                            <label>Código Serviço</label>
                            <select class="form-control" ng-model="parcela.codigoServicoEstabelecimento_id" ng-disabled="ModoConsultaPA"
                                    ng-options="o.Value as o.Text for o in ListaCodigoServicos" >
                                <option required value="">--Selecione--</option>
                            </select>
                        </div>
                        <div class='col-md-4 form-group'>
                            <label>Banco</label>
                            <select class="form-control" ng-model="parcela.banco_id" ng-disabled="ModoConsultaPA"
                                    ng-options="o.Value as o.Text for o in ListaBancos" required>
                                <option value="">--Selecione--</option>
                            </select>
                        </div>
                        <div class='col-md-4 form-group'>
                            <label>Gera Nota Autómatico</label>
                            <select class="form-control" ng-model="parcela.NFAutomatico" ng-disabled="ModoConsultaPA"
                                    ng-options="o.Value as o.Text for o in ListaSimNao" required>
                                <option value="">--Selecione--</option>
                            </select>
                        </div>


                    </div>
                </div>
                <div class="modal-footer">
                    <div class="alert alert-danger" ng-show="showNOK">
                        {{msgNOK}}
                        <ul>
                            <li ng-repeat="item in Erros">{{item}}</li>
                        </ul>
                    </div>
                    <button class="btn btn-primary" ng-click="ok()" ng-show="!ModoConsultaPA">
                        <span class="glyphicon glyphicon-ok"></span>
                        Salvar
                    </button>
                    <button class="btn btn-default" ng-click="cancel()">
                        <span class="glyphicon glyphicon-log-out"></span>
                        Voltar

                    </button>
                </div>
            </div>
        </script>

        <script type="text/ng-template" id="modalParcelaMulti.html">
            <div>
                <div class="modal-header">
                    <h3>Multipla Alteração de Parcelas</h3>
                </div>
                <div class="modal-body">
                    <dir-pagination-controls max-size="10" boundary-links="true" pagination-id="gridSecundario"></dir-pagination-controls>
                    <table class="table table-bordered">
                        <thead>
                            <tr>
                                <th>
                                    Parcela
                                </th>
                                <th>
                                    Código
                                </th>
                                <th>
                                    Descrição
                                </th>
                                <th>
                                    Valor
                                </th>
                                <th>
                                    Data
                                </th>
                                <th>
                                    Vencimento
                                </th>
                                <th>
                                    Operações
                                </th>
                                <th style="width:100px;">
                                    Tipo Faturamento
                                </th>
                                <th>
                                    Código Serviço
                                </th>
