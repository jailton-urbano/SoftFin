                                <td style="width:30px;">

                                    <input ng-disabled="ModoConsultaPA" type="text" ng-model="item.parcela" class="form-control" required ng-maxlength="4" />
                                </td>
                                <td style="width:50px;">
                                    <input ng-disabled="ModoConsultaPA" type="text" ng-model="item.Codigo" class="form-control" required ng-maxlength="100" />
                                    <button ng-click="CopiaResultado($index, item.Codigo,'Codigo')">
                                        <span class="glyphicon glyphicon-arrow-down"></span>
                                    </button>
                                </td>

                                <td style="width:100px;">
                                    <input ng-disabled="ModoConsultaPA" type="text" ng-model="item.descricao" class="form-control" required ng-maxlength="100" />
                                    <button ng-click="CopiaResultado($index, item.descricao,'descricao')">
                                        <span class="glyphicon glyphicon-arrow-down"></span>
                                    </button>
                                </td>
                                <td style="width:100px;">
                                    <input ng-disabled="ModoConsultaPA" type="text" ng-model="item.valor" class="form-control" required ng-maxlength="100" ui-number-mask="2" />
                                    <button ng-click="CopiaResultado($index, item.valor,'valor')">
                                        <span class="glyphicon glyphicon-arrow-down"></span>
                                    </button>

                                </td>
                                <td style="width:100px;" datetimez ng-model="item.data">

                                    <input data-format="dd/MM/yyyy" type="text" class="form-control" ng-model="item.data" required ui-date-mask ng-disabled="ModoConsultaPA" />
                                    <span class="add-on">
                                        <i data-time-icon="icon-time" data-date-icon="icon-calendar"></i>
                                    </span>
                                </td>
                                <td style="width:100px;" datetimez ng-model="item.DataVencimento">

                                    <input data-format="dd/MM/yyyy" type="text" class="form-control" ng-model="item.DataVencimento" required ui-date-mask ng-disabled="ModoConsultaPA" />
                                    <span class="add-on">
                                        <i data-time-icon="icon-time" data-date-icon="icon-calendar"></i>
                                    </span>
                                </td>
                                <td style="width:140px;">
                                    <select class="form-control" ng-model="item.operacao_id" ng-disabled="ModoConsultaPA"
                                            ng-options="o.Value as o.Text for o in ListaOperacoes" required>
                                        <option value="">--Selecione--</option>
                                    </select>
                                    <button ng-click="CopiaResultado($index, item.operacao_id,'operacao_id')">
                                        <span class="glyphicon glyphicon-arrow-down"></span>
                                    </button>

                                </td>
                                <td style="width:100px;">
                                    <select class="form-control" ng-model="item.TipoFaturamento" ng-disabled="ModoConsultaPA"
                                            ng-options="o.Value as o.Text for o in ListaTipoFaturamento" required>
                                        <option value="">--Selecione--</option>
                                    </select>
                                    <button ng-click="CopiaResultado($index, item.TipoFaturamento,'TipoFaturamento')">
                                        <span class="glyphicon glyphicon-arrow-down"></span>
                                    </button>

                                </td>
                                <td style="width:120px;">

                                    <select class="form-control" ng-model="item.codigoServicoEstabelecimento_id" ng-disabled="ModoConsultaPA"
                                            ng-options="o.Value as o.Text.substring(0,50) for o in ListaCodigoServicos" required>
                                        <option value="">--Selecione--</option>
                                    </select>
                                    <button ng-click="CopiaResultado($index, item.codigoServicoEstabelecimento_id,'codigoServicoEstabelecimento_id')">
                                        <span class="glyphicon glyphicon-arrow-down"></span>
                                    </button>
                                </td>
                                <td style="width:120px;">

                                    <select class="form-control" ng-model="item.banco_id" ng-disabled="ModoConsultaPA"
                                            ng-options="o.Value as o.Text for o in ListaBancos" required>
                                        <option value="">--Selecione--</option>
                                    </select>
                                    <button ng-click="CopiaResultado($index, item.banco_id,'banco_id')">
                                        <span class="glyphicon glyphicon-arrow-down"></span>
                                    </button>
                                </td>
                                <td style="width: 60px;">
                                    <select class="form-control" ng-model="item.NFAutomatico" ng-disabled="ModoConsultaPA"
                                            ng-options="o.Value as o.Text for o in ListaSimNao" required>
                                        <option value="">--Selecione--</option>
                                    </select>
                                    <button ng-click="CopiaResultado($index, item.NFAutomatico,'NFAutomatico')">
                                        <span class="glyphicon glyphicon-arrow-down"></span>
                                    </button>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                    <dir-pagination-controls max-size="10" boundary-links="true" pagination-id="gridSecundario"></dir-pagination-controls>
                    <div class="alert alert-danger" ng-show="showMSGPedido">
                        {{msgMSGPedido}}
                    </div>

                </div>
                <div class="modal-footer">
                    <div class="alert alert-danger" ng-show="showNOK">
                        {{msgNOK}}
                        <ul>
                            <li ng-repeat="item in Erros track by $index">{{item}}</li>
                        </ul>
                    </div>
                    <button class="btn btn-primary" ng-click="ok()" ng-show="!ModoConsultaPA">
                        <span class="glyphicon glyphicon-ok"></span>
                        Salvar
                    </button>
                    <button class="btn btn-default" ng-click="cancel()">
                        <span class="glyphicon glyphicon-log-out"></span>
                        Voltar
                    </button>
                </div>
            </div>
        </script>

