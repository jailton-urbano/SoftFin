                                    <td style="width: 40px;">
                                        <button style="margin-top: 4px; border-right-width: 0px; padding-right: 50px;"
                                                ng-click="DetalharCI(item)"
                                                class="btn btn-primary">
                                            <span class="glyphicon glyphicon-list-alt"></span> Detalhar
                                        </button>
                                        <button style="margin-top: 4px; border-right-width: 0px; padding-right: 68px;"
                                                ng-click="AlterarCI(item)"
                                                class="btn btn-primary">
                                            <span class="glyphicon glyphicon-edit"></span> Editar
                                        </button>
                                        <button style="margin-top: 4px;border-right-width: 0px; padding-right: 63px;"
                                                type="button"
                                                ng-click="ExcluirContratoItem(item)"
                                                class="btn btn-danger">
                                            <span class="glyphicon glyphicon-remove"></span> Excluir
                                        </button>
                                    </td>

                                    <td>
                                        <b>
                                            <span ng-click="ordenar('pedido')"> Codigo/Pedido </span> / <span ng-click="ordenar('valor')">Valor </span>
                                        </b>
                                        <br />
                                        {{item.pedido}} / {{item.valor |number:2}}
                                        <br />
                                        <table class="table table-bordered">
                                            <thead>
                                                <tr>
                                                    <th>
                                                        Unidade
                                                    </th>
                                                    <th>
                                                        Descrição
                                                    </th>
                                                    <th>
                                                        Valor
                                                    </th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr dir-paginate="item2 in item.ListaItemUnidades |itemsPerPage:1000" pagination-id="gridSecundario">
                                                    <td>{{item2.Unidade_Desc}}</td>
                                                    <td>
                                                        {{item2.Descricao}}
                                                    </td>
                                                    <td>{{item2.Valor | number:2}}</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </td>
                                    <td>
                                        <div class="panel panel-success">
                                            <div class="panel-heading">Parcelas</div>
                                            <dir-pagination-controls max-size="10" boundary-links="true" pagination-id="gridParcelas"></dir-pagination-controls>
                                            <table class="table table-bordered">
                                                <thead>
                                                    <tr>
                                                        <th>
                                                            <button style="border-right-width: 0px; padding-right: 49px;"
                                                                    ng-click="LoadListaParcelas(item)"
                                                                    class="btn btn-warning">
                                                                <span class="glyphicon glyphicon-refresh"></span> Atualizar
                                                            </button>
                                                            <button ng-show="!ModoConsulta" style="margin-top: 4px; padding-right: 72px;"
                                                                    ng-click="IncluirPA(item)"
                                                                    class="btn btn-primary">
                                                                <span class="glyphicon glyphicon-plus"></span> Novo
                                                            </button>
                                                            <button ng-show="!ModoConsulta" style="margin-top: 4px; padding-right: 12px;"
                                                                    ng-click="EditPAMulti(item)"
                                                                    class="btn btn-primary">
                                                                <span class="glyphicon glyphicon-edit"></span> Edição Multipla
                                                            </button>
                                                        </th>
                                                        <th>Descrição</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr dir-paginate="itemParcela in item.ListaParcelas|filter:procurarContratosItens|itemsPerPage:4|orderBy:sortKey3:reverse3" class="{{item.active}}" pagination-id="gridParcelas">
                                                        <td style="width: 40px;">
                                                            <button style="margin-top: 4px; border-right-width: 0px; padding-right: 50px;"
                                                                    ng-click="DetalhaPA(itemParcela,item)"
                                                                    class="btn btn-primary">
                                                                <span class="glyphicon glyphicon-list-alt"></span> Detalhar
                                                            </button>
                                                            <button style="margin-top: 4px; border-right-width: 0px; padding-right: 68px;"
                                                                    ng-disabled="(itemParcela.status == 'Emitida') || (itemParcela.status == 'Liberada')"
                                                                    ng-click="AlterarPA(itemParcela,item)"
                                                                    class="btn btn-primary">
                                                                <span class="glyphicon glyphicon-edit"></span> Editar
                                                            </button>
                                                            <button style="margin-top: 4px;border-right-width: 0px; padding-right: 63px;"
                                                                    type="button"
                                                                    ng-disabled="(itemParcela.status == 'Emitida') || (itemParcela.status == 'Liberada')"
                                                                    ng-click="ExcluirParcela(itemParcela,item)"
                                                                    class="btn btn-danger">
                                                                <span class="glyphicon glyphicon-remove"></span> Excluir
                                                            </button>
                                                        </td>

                                                        <td>
                                                            <b>
                                                                <span ng-click="ordenar3('parcela')"> Parcela </span> / <span>Descrição </span> <br />
                                                            </b>

                                                            {{itemParcela.parcela}} / {{itemParcela.descricao}} <br />

                                                            <b>
                                                                <span> Data </span> / <span ng-click="ordenar3('DataVencimento')"> Vencimento </span>
                                                            </b>
                                                            <br />
                                                            {{itemParcela.data | date:'dd/MM/yyyy'}} / {{itemParcela.DataVencimento | date:'dd/MM/yyyy'}}
                                                            <br />

                                                            <b>
                                                                <span>Valor </span> / <span ng-click="ordenar3('status')">Status </span>
                                                            </b>
                                                            <br />
                                                            {{itemParcela.valor |number:2}} / {{itemParcela.status}}
                                                        </td>

                                                    </tr>
                                                </tbody>
                                            </table>
                                            <dir-pagination-controls max-size="10" boundary-links="true" pagination-id="gridParcelas"></dir-pagination-controls>
                                        </div>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                        <dir-pagination-controls max-size="10" boundary-links="true" pagination-id="gridContratosItens"></dir-pagination-controls>

                    </div>

                </div>
            </div>
        </div>

    </div>
</div>




